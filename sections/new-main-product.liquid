    <script>
      var benefits_swiper;
      var initSwiper = function () {
        var screenWidth = window.innerWidth || document.documentElement.clientWidth;

        if (screenWidth >= 1360) {
          if (!benefits_swiper) {
            benefits_swiper = new Swiper('.benefits_swiper_wrapper', {
              slidesPerView: 2.7,
              spaceBetween: 12,
            });
          }
        } else {
          if (benefits_swiper) {
            benefits_swiper.destroy(true, true);
            benefits_swiper = undefined;
          }
        }
      };

      initSwiper();

      // Re-check on window resize
      window.addEventListener('resize', initSwiper);

      document.querySelector('.custom-product__info-btn').addEventListener('click', () => {
        document.querySelector('.product-form__submit').click();
      });
    </script>
